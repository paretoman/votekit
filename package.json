{
  "name": "@paretoman/votekit",
  "version": "1.0.0",
  "description": "An explorable sandbox that shows how different voting methods work using a spatial opinion model.",
  "browser": "src\\sandbox\\addSandboxes.js",
  "type": "module",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "doc": "jsdoc -c jsdoc.json",
    "build": "npm run parcel-build && npm run copy1 && npm run copy2 && npm run copy3",
    "parcel-build": "parcel build src/addSandboxes/addSandboxes.js --dist-dir dist/src/sandbox",
    "copy1": "copyfiles -u 1 \"src/**/*.{css,html}\" \"dist/src/\"",
    "copy2": "copyfiles -u 1 \"test/**/*.*\" \"dist/test/\"",
    "copy3": "copyfiles -u 1 \"tutorial/**/*.*\" \"dist/tutorial/\"",
    "build-n-doc": "npm run build && npm run doc",
    "publish-docs": "cd docs && git add . && git commit && git push -u",
    "snowpack": "snowpack build",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paretoman/votekit.git"
  },
  "author": {
    "name": "paretoman",
    "url": "https://paretoman.github.io"
  },
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/paretoman/votekit/issues"
  },
  "homepage": "https://paretoman.github.io/votekit",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@mithrandirii/canvas2svg": "^1.0.24",
    "@tweenjs/tween.js": "^18.6.4",
    "copyfiles": "^2.4.1",
    "d3-contour": "^3.0.1",
    "d3-delaunay": "^6.0.2",
    "d3-geo": "^3.0.1",
    "d3-polygon": "^3.0.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jsdoc": "^3.6.10",
    "parcel": "^2.4.1",
    "process": "^0.11.10",
    "simplex-noise": "^3.0.1",
    "snowpack": "^3.8.8",
    "split-polygon": "^1.0.0"
  },
  "keywords": [
    "explorable",
    "sandbox",
    "voting",
    "voting-methods",
    "social-choice",
    "computational-social-choice",
    "elections",
    "strategy",
    "electoral-methods",
    "plurality",
    "approval",
    "condorcet",
    "instant-runoff",
    "score",
    "star",
    "proportional",
    "proportional-representation",
    "districts",
    "redistricting",
    "single-transferable-vote",
    "monroe",
    "thiele",
    "proportional-approval-voting",
    "phragmen",
    "party-list",
    "party-list-proportional",
    "dHondt",
    "Huntington-Hill",
    "Webster",
    "Sainte-Lague",
    "MMP",
    "OLPR"
  ]
}
