{
  "name": "@paretoman/votekit-compute",
  "version": "0.0.0",
  "description": "Run a spatial election model. Input geometry. Cast votes. Use a social choice function. Collect results. Also combine multiple elections across geography, districts, samplings, and polling.",
  "main": "compute/compute.js",
  "type": "module",
  "scripts": {
    "test": "mocha --reporter list",
    "snowpack": "snowpack build",
    "lint": "eslint .",
    "doc": "jsdoc -c jsdoc.json",
    "check-types": "tsc --noEmit -p ./jsconfig.json",
    "check-types-silent": "tsc-silent -p ./jsconfig.json --suppress @/src/lib @/snowpack.config.cjs @/test/test.js"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paretoman/votekit.git"
  },
  "author": "paretoman (https://paretoman.github.io)",
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/paretoman/votekit/issues"
  },
  "homepage": "https://paretoman.github.io/votekit",
  "keywords": [
    "voting",
    "voting-methods",
    "social-choice",
    "computational-social-choice",
    "elections",
    "strategy",
    "electoral-methods",
    "plurality",
    "approval",
    "condorcet",
    "instant-runoff",
    "score",
    "star",
    "proportional",
    "proportional-representation",
    "districts",
    "redistricting",
    "single-transferable-vote",
    "monroe",
    "thiele",
    "proportional-approval-voting",
    "phragmen",
    "party-list",
    "party-list-proportional",
    "dHondt",
    "Huntington-Hill",
    "Webster",
    "Sainte-Lague",
    "MMP",
    "OLPR"
  ],
  "devDependencies": {
    "d3-delaunay": "^6.0.2",
    "d3-polygon": "^3.0.1",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^40.0.0",
    "mocha": "^10.2.0",
    "seedrandom": "^3.0.5",
    "simplex-noise": "^3.0.1",
    "snowpack": "^3.8.8",
    "tsc-silent": "^1.2.2",
    "typescript": "^4.9.5"
  }
}
