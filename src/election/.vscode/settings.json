{
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "eslint.validate": [
      "javascript"
    ],
    "javascript.preferences.importModuleSpecifierEnding": "js",
    "cSpell.words": [
      "borda",
      "Buschmann",
      "drawio",
      "Hondt",
      "Lague",
      "linebreak",
      "olpr",
      "phragmen",
      "plusplus",
      "Sainte",
      "Subpage",
      "Subpages",
      "thiele"
    ],
    "emeraldwalk.runonsave": {
      "commands": [
          {
              "match": "\\.js$",
              "cmd": "npm run doc"
          }
      ]
  }
}